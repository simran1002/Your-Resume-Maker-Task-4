<div class="container">
    <p class="text-primary">You are using <%=template%> theme</p>

    <form action="/resume-maker" class="text-primary form" method="POST">
        <h4>Personal Details</h4>
        <div class="form-row">
            <div class="col-md-4-mb-3">
                <label for="Full Name">Full Name</label>
                <input type="text" name="U_name" class="form-control">
            </div>
            <div class="col-md-4-mb-3">
                <label for="Phone Number">Phone No.</label>
                <input type="number" name="U_phone" class="form-control">
            </div>
            <div class="col-md-4-mb-3">
                <label for="E-Mail">Email</label>
                <input type="email" name="U_email" class="form-control">
            </div>
            <div class="col-md-4-mb-3">
                <label for="">Address</label>
                <input type="text" name="U_address" class="form-control">
            </div>
            <div class="col-md-4-mb-3">
                <label for="E-Mail">State</label>
                <input type="text" name="state_name" class="form-control">
            </div>
            <div class="col-md-4-mb-3">
                <label for="E-Mail">City</label>
                <input type="text" name="city_name" class="form-control">
            </div>
            <!-- <div class="col-md-4-mb-3">
                <label for="E-Mail">Linkedin Id</label>
                <input type="text" name="linkedin_id" class="form-control">
            </div> -->
        </div>
        <br>
        <hr>
        <div class="form-group">
            <h4>About</h4>
            <div class="col-md-4-mb-3">
                <label for="">Applying for</label>
                <input type="text" name="U_appling" class="form-control">
            </div>
            <div class="col-md-4-mb-3">
                <label for="">Short-discription</label>
                <input type="text" name="Short_discription" class="form-control">
            </div>
        </div>
        <br>
        <hr>
        <div class="form-row">
            <h4>Work Experience </h4>
            <div class="col-md-4-mb-3">
                <label for="">Time of Employment</label>
                <input type="date" name="time_preiod_1" class="form-control">
            </div>
            <div class="col-md-4-mb-3">
                <label for="">Company Name</label>
                <input type="text" name="post_1" class="form-control">
            </div>
            <!-- <div class="col-md-4-mb-3">
                <label for="">Job Description</label>
                <input type="text" name="job_" class="form-control">
            </div> -->
            <!-- <div class="col-md-4-mb-3">
                <label for="">Time Period:</label>
                <input type="date" name="time_preiod_2" class="form-control">
            </div>
            <div class="col-md-4-mb-3">
                <label for="">Post</label>
                <input type="text" name="post_2" class="form-control">
            </div> -->
        </div>
        <br>
        <hr>
        <div class="form-row">
            <h4>Education</h4>
            <div class="col-md-4-mb-3">
                <label for="">Graduation</label>
                <input type="text" name="Graduation" class="form-control">
            </div>
            <div class="col-md-4-mb-3">
                <label for="">10+2</label>
                <input type="text" name="senior_secondary" class="form-control">
            </div>
            <!-- <div class="col-md-4-mb-3">
                <label for="">exp 3</label>
                <input type="text" name="email" class="form-control">
            </div>
            <div class="col-md-4-mb-3">
                <label for="">date of birth</label>
                <input type="text" name="birth" class="form-control">
            </div> -->
        </div>
        <br>
        <hr>
        <div class="form-group">
            <h4>Projects</h4>
            <div class="col-md-4-mb-3">
                <label for="">Project 1:</label>
                <input type="text" name="project_1" class="form-control">
            </div>
            <div class="col-md-4-mb-3">
                <label for="">Project 2:</label>
                <input type="text" name="project_2" class="form-control">
            </div>
        </div>
        <br>
        <hr>
        <div class="form-row">
            <h4>Skills</h4>
            <div class="col-md-4-mb-3">
                <label for="">Skill 1:</label>
                <input type="text" name="skill_1" class="form-control">
            </div>
            <div class="col-md-4-mb-3">
                <label for="">Skill 2:</label>
                <input type="text" name="skill_2" class="form-control">
            </div>
        </div>
        <!-- <div class="form-group">
            <h4>Interest</h4>
            <textarea name="interest" id="" cols="30" rows="10" class="form-control"></textarea>
        </div> -->
        <div class="form-group">
            <select name="theme" id="" hidden>
                <option value="<%=template%>"><%=template%></option>
            </select>
        </div>

        <br>
        <hr>

        <button class="btn btn-primary" type="submit">Submit</button>

    </form>

</div>

<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js"></script>

